{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Scan creation</h1>

    {% if errors is defined %}
        <h3>The author has the following errors</h3>
        <ul>
            {% for error in errors %}
                <li>{{ error.message }}</li>
            {% endfor %}
        </ul>
    {% endif %}

    {{ form(form) }}
{% endblock %}
