{% extends "FOSUserBundle::layout.html.twig" %}


{% block fos_user_content %}
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4 content scan-new">

                <h1 class="inverse">Scan erfassen</h1>

                {% if errors is defined %}
                    <h3>The author has the following errors</h3>
                    <ul>
                        {% for error in errors %}
                            <li>{{ error.message }}</li>
                        {% endfor %}
                    </ul>
                {% endif %}

                <div class="alert alert-success" role="alert">
                    Erfolgreich!
                </div>
            </div>
        </div>
    </div>
{% endblock %}
