{% extends "FOSUserBundle::layout.html.twig" %}

{% block body -%}
    <h1>Network</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Netrange</th>
                <td>{{ entity.netRange }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('network') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('network_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
